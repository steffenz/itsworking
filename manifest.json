{
  "name": "It's Working",
  "description": "Fixes flaws and annoyances with Itslearning.",
  "manifest_version": 2,
  "version": "1.0",
  "web_accessible_resources": ["loading.gif"],
  "icons": { "16": "16.png",
           "48": "48.png",
          "128": "128.png" },
  "content_scripts": [
    {
      
      "matches": ["http://www.itslearning.com/*","https://www.itslearning.com/*","http://www.itslearning.com/*"],
      "js": ["jquery.js", "code.js"],
      "run_at": "document_start"
    }
  ]
}